package assignment3;

public class Employee {
	String firstName;
	String lastName;
	int age;
	double salary;
	
	public Employee(){
		
	}
	
	public Employee(String firstName, String lastName, int age, double salary){
		this.firstName = firstName;
		this.lastName = lastName;
		this.age = age;
		this.salary = salary;
	}
	
	Employee e1 = new Employee();
	Employee e2 = new Employee();

}
