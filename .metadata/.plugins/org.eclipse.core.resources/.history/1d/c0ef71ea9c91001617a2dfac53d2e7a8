package apexDay2;
import java.util.*;
public class Exercise6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n;
		int factorial = 1;
		
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter the number you want to know factorial of : ");
		n = sc.nextInt();
		for( int i=n; i>=1; n-- ){
			factorial *= n*(n-1);
			
		}
		
		System.out.println(factorial);
	}

}
