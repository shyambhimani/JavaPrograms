package polymorphism;

public class Car {
	
	public run(){
		
	}
	
	public static void main(String args[]) {
		
		// Reference variable of parent class(Car) is referring to the object of Child class(Mercedes)
		Polymorphism c1 = new Mercedes();
		
		c1.run();

	}

	/**
	 * 
	 */
	public void run() {
		// TODO Auto-generated method stub
		
	}

	// Super Class Car
	class Car extends Polymorphism{

		// Run method of Super class
		public void run() {

			System.out.println("Car is running");

		}
	}

	// Sub Class Mercedes inherit to Car class
	class Mercedes extends Polymorphism {

		// Override run method (Runtime Polymorphism)
		@Override
		public void run() {

			System.out.println("Mercedes is running");

		}

	}

	// Another sub-class BMW inherit to Super class Car
	class BMW extends Polymorphism {

		// Override method run
		public void run() {

			System.out.println("BMW is running");
		}
	}

	
}
