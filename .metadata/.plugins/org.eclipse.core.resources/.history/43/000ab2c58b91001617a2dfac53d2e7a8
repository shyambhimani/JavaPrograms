package apexDay2;
import java.util.Scanner;
public class Exercise4 {
	
	public static void main (String[] args){
		
		int sub1, sub2, sub3, sub4, sub5, sub6;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter number of student : ");
		
		for (int i = sc.nextInt(); i>0; i++);
		
		System.out.println("Please enter marks of 1st subject" );
		sub1 = sc.nextInt();
		System.out.println("Please enter marks of 2nd subject" );
		sub2 = sc.nextInt();
		System.out.println("Please enter marks of 3rd subject" );
		sub3 = sc.nextInt();
		System.out.println("Please enter marks of 4th subject" );
		sub4 = sc.nextInt();
		System.out.println("Please enter marks of 5th subject" );
		sub5 = sc.nextInt();
		System.out.println("Please enter marks of 6th subject" );
		sub6 = sc.nextInt();
		System.out.println(sub6);
		
	}

}
