package apexDay2;
import java.util.*;
public class Exercise6 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int n, i, factorial;
		Scanner sc = new Scanner(System.in);
		System.out.println("Please enter the number you want to know factorial of : ");
		n = sc.nextInt();
		for( i = 1; i<=n; i++ ){
			factorial = i*i;
			
			
		}
		
		System.out.println(factorial);
	}

}
