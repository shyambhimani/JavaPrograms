package com.apex.collection;

import java.math.BigInteger;

public class BIExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		BigInteger b = new BigInteger("2328645386458345835845853358585757572778673486346751327");
		BigInteger b1 = new BigInteger("232864538645834583584585335858575757");
		b.add(b1);
		System.out.println(b);
	}

}
